{
  "name": "kiratakipasistan",
  "version": "1.0.0",
  "private": true,
  "author": {
    "name": "NİBGAT®",
    "email": "contact@nibgat.com",
    "url": "https://www.nibgat.com/"
  },
  "dependencies": {
    "@apollo/react-hooks": "^4.0.0",
    "@fortawesome/fontawesome": "1.1.8",
    "@fortawesome/fontawesome-svg-core": "1.2.22",
    "@fortawesome/free-solid-svg-icons": "5.10.2",
    "@fortawesome/react-fontawesome": "0.1.4",
    "apollo-boost": "^0.4.9",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-upload-client": "^14.1.3",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "babel-preset-stage-1": "6.24.1",
    "graphql": "^15.5.0",
    "jquery": "3.4.1",
    "js-cookie": "2.2.1",
    "js-md5": "0.7.3",
    "lightbox-react": "^0.3.8",
    "moment": "2.24.0",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-electron-contextmenu": "1.0.0",
    "react-jss": "10.5.0",
    "react-router-dom": "5.1.2",
    "react-scripts": "3.0.1",
    "validator": "^13.5.2"
  },
  "main": "public/electron.js",
  "homepage": "./",
  "scripts": {
    "react-start": "react-scripts start",
    "react-build": "react-scripts build",
    "react-test": "react-scripts test --env=jsdom",
    "react-eject": "react-scripts eject",
    "build": "yarn react-build && electron-builder --publish=always",
    "electron-dev": "concurrently \"cross-env BROWSER=none npm run react-start\" \"wait-on http://localhost:3000 && electron .\"",
    "desktop": "npm run electron-dev"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "4.1.0",
    "cross-env": "5.2.0",
    "electron": "5.0.1",
    "electron-build": "0.0.3",
    "electron-is-dev": "1.1.0",
    "wait-on": "3.2.0"
  },
  "build": {
    "appId": "com.hifasoft.kiratakipasistan",
    "productName": "Kira Takip Asistan",
    "copyright": "Copyright © 2020 ${author}",
    "mac": {
      "category": "public.app-category.utilities"
    },
    "asar": false,
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "public"
    }
  }
}
